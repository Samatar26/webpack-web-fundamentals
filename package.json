  "bugs": {
    "url": "https://github.com/Samatar26/webpack-web-fundamentals/issues"
  },
  "homepage": "https://github.com/Samatar26/webpack-web-fundamentals#readme",
  "devDependencies": {
    "bundle-buddy-webpack-plugin": "^0.3.0",
    "html-webpack-plugin": "^2.30.1",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  }
}
